{
  "manifest_version": 3,
  "name": "CompanyName Widget",
  "version": "1.0",
  "description": "",
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*","https://in.indeed.com/*" ],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "web_accessible_resources": [
  {
    "resources": ["icon.png"],
    "matches": [
        "https://www.linkedin.com/*",
        "https://*.indeed.com/*",
        "https://www.glassdoor.com/*",
        "https://www.glassdoor.co.in/*"
      ],
    "use_dynamic_url": true
  }
]
}

