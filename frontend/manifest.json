{
  "manifest_version": 3,
  "name": "CompanyName Widget",
  "version": "1.0",
  "description": "",
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*" ],
      "js": ["content.js"],
      "css": ["content.css"]
