{
  "manifest_version": 3,
  "name": "Wynisco Widget",
  "version": "1.0",
  "description": "Posts Job listings",
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*",
        "https://*.indeed.com/*"
      ],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "web_accessible_resources": [
  {
    "resources": ["icon.png"],
    "matches": [
        "https://www.linkedin.com/*",
        "https://*.indeed.com/*"
      ],
    "use_dynamic_url": true
  }
]
}

